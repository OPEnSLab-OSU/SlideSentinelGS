  
{
    "presets": [
        [ 
            "@babel/env", 
            { 
                "useBuiltIns": "usage", 
                "corejs": 3,
                "targets": {
                    "esmodules": true
                },
                // will be default in babel 8
                "bugfixes": true
            } 
        ],
        "@babel/typescript", 
        "gas",
        "minify"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread"
    ]
}